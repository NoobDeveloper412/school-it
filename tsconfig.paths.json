{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@interfaces/*": ["./src/interfaces/*"],
      // Suppress 'Cannot import type declaration files.' (node_modules/@types)
      "@typings/*": ["./src/typings/*"],
      "@redux/*": ["./src/redux/*"],
      "@reducers/*": ["./src/redux/reducers/*"],
      "@actions/*": ["./src/redux/actions*"],
      "@contants": ["./src/config/constants"],
      "@components/*": ["./src/components/*"],
      "@utils/*": ["./src/utils/*"],
      "@theme/*": ["./src/theme/*"],
      "@config/*": ["./src/config/*"],
      "@repositories/*": ["./src/repositories/*"]
    }
  }
}
